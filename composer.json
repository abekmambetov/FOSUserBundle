{
    "name": "friendsofsymfony/user-bundle",
    "type": "symfony-bundle",
    "description": "Symfony FOSUserBundle",
    "keywords": [
        "User management"
    ],
    "homepage": "http://friendsofsymfony.github.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Coevoet",
            "email": "stof@notk.org"
        },
        {
            "name": "Thibault Duplessis"
        },
        {
            "name": "FriendsOfSymfony Community",
            "homepage": "https://github.com/friendsofsymfony/FOSUserBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0",
        "ext-dom": "*",
        "ext-json": "*",
        "symfony/config": "^2.8 || ^5.0",
        "symfony/dependency-injection": "^2.8 || ^5.0",
        "symfony/event-dispatcher": "^2.8 || ^5.0",
        "symfony/event-dispatcher-contracts": "^1.1 || ^2.0",
        "symfony/form": "^2.8 || ^5.0",
        "symfony/framework-bundle": "^2.8 || ^5.0",
        "symfony/http-foundation": "^2.8 || ^5.0",
        "symfony/http-kernel": "^2.8 || ^5.0",
        "symfony/options-resolver": "^2.8 || ^5.0",
        "symfony/routing": "^2.8 || ^5.0",
        "symfony/security-bundle": "^2.8 || ^5.0",
        "symfony/security-core": "^2.8 || ^5.0",
        "symfony/templating": "^2.8 || ^5.0",
        "symfony/translation": "^2.8 || ^5.0",
        "symfony/twig-bundle": "^2.8 || ^5.0",
        "symfony/validator": "^2.8 || ^5.0",
        "twig/twig": "^1.34 || ^2.4 || ^3.0"
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<1.3",
        "doctrine/persistence": "<1.3",
        "symfony/doctrine-bridge": "<2.8"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.0.2",
        "swiftmailer/swiftmailer": "^4.3 || ^5.0 || ^6.0",
        "symfony/console": "^2.8 || ^5.0",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/yaml": "^2.8 || ^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "FOS\\UserBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "FOS\\UserBundle\\Tests\\": "/Tests/"
        }
    },
    "support": {
        "docs": "https://symfony.com/doc/master/bundles/FOSUserBundle/index.html"
    }
}
